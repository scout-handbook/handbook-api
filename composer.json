{
	"name": "genabitu/handbookapi",
	"description": "A back-end API for creating a course handbook connected to SkautIS",
	"version": "0.9.0",
	"type": "project",
	"readme": "README.md",
	"license": "MIT",
	"authors": [
		{
			"name": "GenaBitu",
			"email": "genabitu@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/GenaBitu/HandbookAPI/issues",
		"source": "https://github.com/GenaBitu/HandbookAPI",
		"docs": "https://genabitu.github.io/HandbookAPI/"
	},
	"require": {
		"php": "^7.1",
		"cebe/markdown": "^1.2",
		"skautis/skautis": "^2.1",
		"ramsey/uuid": "^3.6",
		"mpdf/mpdf": "^7.0",
		"bacon/bacon-qr-code": "^2.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.3",
		"wimg/php-compatibility": "^9.0",
		"phpmd/phpmd": "^2.6",
		"phpunit/phpunit": "^7.4",
		"phpunit/dbunit": "^4.0",
		"phpstan/phpstan": "^0.10.6"
	},
	"scripts": {
		"phpcs": "vendor/bin/phpcs",
		"phpmd": "vendor/bin/phpmd --ignore-violations-on-exit v0.9 text phpmd.xml",
		"phpunit": "vendor/bin/phpunit",
		"lint": [
			"@phpcs",
			"@phpmd"
		],
		"unit": [
			"@phpunit"
		],
		"ci": [
			"@lint",
			"@unit"
		]
	}
}
