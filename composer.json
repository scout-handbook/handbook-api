{
	"name": "genabitu/handbookapi",
	"description": "A back-end API for creating a course handbook connected to SkautIS",
	"version": "0.9.0",
	"type": "project",
	"readme": "README.md",
	"license": "MIT",
	"authors": [
		{
			"name": "GenaBitu",
			"email": "genabitu@gmail.com"
		}
	],
	"support": {
		"issues": "https://gitlab.com/skaut-handbook/handbook-api/issues",
		"source": "https://gitlab.com/skaut-handbook/handbook-api",
		"docs": "https://skaut-handbook.gitlab.io/handbook-api/"
	},
	"require": {
		"php": "^7.1",
		"cebe/markdown": "^1.2",
		"skautis/skautis": "^2.1",
		"ramsey/uuid": "^3.6",
		"mpdf/mpdf": "^7.0",
		"bacon/bacon-qr-code": "^2.0"
	},
	"require-dev": {
		"squizlabs/php_codesniffer": "^3.3",
		"phpmd/phpmd": "^2.6",
		"phpunit/phpunit": "^7.5",
		"phpunit/dbunit": "^4.0",
		"phpcompatibility/php-compatibility": "^9.0"
	},
	"autoload": {
		"files": [
			"HandbookAPI/v0_9/Lesson_cmp.php",
			"HandbookAPI/v0_9/LessonContainer_cmp.php",
			"HandbookAPI/v0_9/roleTry.php",
			"HandbookAPI/v0_9/skautisTry.php"
		],
		"psr-4": {
			"Skaut\\": "./"
		}
	},
	"scripts": {
		"phpcs": "vendor/bin/phpcs",
		"phpmd": "vendor/bin/phpmd --ignore-violations-on-exit v0.9 text phpmd.xml",
		"phpunit": "vendor/bin/phpunit",
		"lint": [
			"@phpcs",
			"@phpmd"
		],
		"unit": [
			"@phpunit"
		],
		"ci": [
			"@lint",
			"@unit"
		]
	}
}
