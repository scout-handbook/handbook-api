parameters:
  level: max
  paths:
    - .
  fileExtensions:
    - php
    - sample
  excludes_analyse:
    - vendor
    - tests
  bootstrap: bootstrap.php
  ignoreErrors:
    - '#Undefined variable: \$CONFIG#'
    - '#Undefined variable: \$[a-zA-Z]+Endpoint#'
    - '#Access to an undefined property Skautis\\Skautis::\$[a-zA-Z]+.#'
